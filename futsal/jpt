for futsal:
users table:

CREATE TABLE futsal420.users (S.N INT(11) NOT NULL AUTO_INCREMENT , Username VARCHAR(30) NOT NULL , Password VARCHAR(255) NOT NULL , Date DATETIME NOT NULL DEFAULT CURRENT_TIMESTAMP , PRIMARY KEY (S.N)) ENGINE = InnoDB;

reservation table:

CREATE TABLE `futsal420`.`reservation123` (`S.N` INT(11) NOT NULL AUTO_INCREMENT , `Fname` VARCHAR(15) NOT NULL , `Lname` VARCHAR(15) NOT NULL , `E-mail` VARCHAR(255) NOT NULL , `Contact` VARCHAR(14) NOT NULL , `Date` DATE NOT NULL , `TSlot` VARCHAR(15) NOT NULL , `Venue` VARCHAR(30) NOT NULL , PRIMARY KEY (`S.N`)) ENGINE = InnoDB;
